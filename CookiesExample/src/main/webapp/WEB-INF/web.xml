<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd" id="WebApp_ID" version="5.0">
  <display-name>CookiesExample</display-name>
  <servlet>
  	<servlet-name>Login</servlet-name>
  	<servlet-class>UserInteractions.LoginUser</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>Logout</servlet-name>
  	<servlet-class>UserInteractions.LogoutUser</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>CheckCookies</servlet-name>
  	<servlet-class>UserInteractions.CheckCookies</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>Login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>Logout</servlet-name>
  	<url-pattern>/logout</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>CheckCookies</servlet-name>
  	<url-pattern>/checkCookies</url-pattern>
  
  </servlet-mapping>
</web-app>